    "editor.tokenColorCustomizations": {
        
        "[*Light*]": {
            "textMateRules": [
                {
                    "scope": "ref.matchtext",
                    "settings": {
                        "foreground": "#000"
                    }
                }
            ]
        },
        "[*Dark*]": {
            "textMateRules": [
                {
                    "scope": "ref.matchtext",
                    "settings": {
                        "foreground": "#fff"
                    }
                }
            ]
        },
        "textMateRules": [
            {
                "scope": "variable.other.property.cpp",
                "settings": {
                    "foreground": "#fd6179"
                },
            },
            {
                "scope": "variable.other.property.static.cpp",
                "settings": {
                    "foreground": "#ff3150"
                },
            },
            {
                "scope": "entity.name.function.preprocessor.cpp",
                "settings": {
                    "foreground": "#ff15e0"
                },
            }
        ]
    },
    "github.copilot.enable": {
        "*": false
    }
}